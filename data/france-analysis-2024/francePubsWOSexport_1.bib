@article{ WOS:000295369100014,
Author = {Lambs, Luc and Leopold, Audrey and Zeller, Bernd and Herteman, Melanie
   and Fromard, Francois},
Title = {Tracing sewage water by <SUP>15</SUP>N in a mangrove ecosystem to test
   its bioremediation ability},
