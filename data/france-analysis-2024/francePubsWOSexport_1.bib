Title = {Lifetime extension for large offshore wind farms: Is it enough to
   reassess fatigue for selected design positions?},
Booktitle = {14TH DEEP SEA OFFSHORE WIND R\&D CONFERENCE, EERA DEEPWIND'2017},
Series = {Energy Procedia},
Year = {2017},
