@article{ WOS:000428532600003,
Author = {Pascal, Nicolas and Brathwaite, Angelique and Brander, Luke and Seidl,
   Andrew and Philip, Maxime and Clua, Eric},
Title = {Evidence of economic benefits for public investment in MPAs},
