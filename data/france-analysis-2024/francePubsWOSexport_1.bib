@article{ WOS:000700695300001,
Author = {Arai, Hironori and Inubushi, Kazuyuki and Chiu, Chih-Yu},
Title = {Dynamics of Methane in Mangrove Forest: Will It Worsen with Decreasing
   Mangrove Forests?},
