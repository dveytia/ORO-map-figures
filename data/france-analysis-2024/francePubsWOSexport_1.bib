@article{ WOS:000359810400008,
Author = {Guidi, Lionel and Legendre, Louis and Reygondeau, Gabriel and Uitz,
   Julia and Stemmann, Lars and Henson, Stephanie A.},
Title = {A new look at ocean carbon remineralization for estimating deepwater
   sequestration},
