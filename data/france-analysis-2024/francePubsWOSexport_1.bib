@inproceedings{ WOS:000419147306017,
Author = {Gastine, M. and Berenblyum, R. and Czernichowski-Lauriol, I. and de
   Dios, J. C. and Audigane, P. and Hladik, V. and Poulsen, N. and
   Vercelli, S. and Vincent, C. and Wildenborg, T.},
Editor = {Dixon, T and Laloui, L and Twinning, S},
Title = {Enabling onshore CO<sub>2</sub> storage in Europe: fostering
   international cooperation around pilot and test sites},
