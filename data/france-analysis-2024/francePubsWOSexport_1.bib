@article{ WOS:000896233200001,
Author = {Ikhennicheu, Maria and Blanc, Arthur and Danglade, Benoat and
   Jean-Christophe, Gilloteaux},
Title = {OrcaFlex Modelling of a Multi-Body Floating Solar Island Subjected to
   Waves},
