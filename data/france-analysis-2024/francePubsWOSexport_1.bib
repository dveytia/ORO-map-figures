@article{ WOS:000294700900050,
Author = {Gal, Frederick and Michel, Brach and Gilles, Braibant and Frederic,
   Jouin and Karine, Michel},
Title = {CO<sub>2</sub> escapes in the Laacher See region, East Eifel, Germany:
   Application of natural analogue onshore and offshore geochemical
   monitoring},
