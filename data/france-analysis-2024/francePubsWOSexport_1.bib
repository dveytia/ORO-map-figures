@article{ WOS:000418644700031,
Author = {Lo Brutto, Ottavio A. and Barakat, Mahmoud R. and Guillou, Sylvain S.
   and Thiebot, Jerome and Gualous, Hamid},
Title = {Influence of the Wake Effect on Electrical Dynamics of Commercial Tidal
   Farms: Application to the Alderney Race (France)},
Journal = {IEEE TRANSACTIONS ON SUSTAINABLE ENERGY},
