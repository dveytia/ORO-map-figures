@article{ WOS:000490757500102,
Author = {Monsinjon, Jonathan R. and Wyneken, Jeanette and Rusenko, Kirt and
   Lopez-Mendilaharsu, Milagros and Lara, Paulo and Santos, Alexsandro and
   dei Marcovaldi, Maria A. G. and Fuentes, Mariana M. P. B. and Kaska,
   Yakup and Tucek, Jenny and Nel, Ronel and William, Kristina L. and
   LeBlanc, Anne-Marie and Rostal, David and Guillon, Jean-Michel and
   Girondot, Marc},
Title = {The climatic debt of loggerhead sea turtle populations in a warming
   world},
Journal = {ECOLOGICAL INDICATORS},
Year = {2019},
Volume = {107},
Month = {DEC},
Abstract = {Phenological shifts, by initiating reproductive events earlier, in
   response to advanced seasonal warming is one of the most striking
   effects currently observed in wild populations. For sea turtles,
   phenological adjustment to warming conditions could be the most
   effective short-term adaptation option against climate change. We
   calculated future phenological changes required in seven important
   loggerhead (Caretta caretta) nesting populations to continue achieving a
   high hatching success and a sex ratio that lies within current ranges.
   Considering temperature-mediated phenological changes, we found that
   most populations (six out of seven) will not be able to keep pace with a
   warming climate. Under an optimistic climate warming scenario (RCP4.5),
   these populations will face a climatic debt, that is, a difference
   between required and expected phenological changes, and warming will
   substantially reduce hatching success and induce a feminization of
   hatchlings, which may jeopardize their reproductive sustainability. Our
   approach offers the possibility to quantify the efficiency of
   phenological shifts in oviparous reptiles by considering physiological,
   developmental and phenological processes.},
