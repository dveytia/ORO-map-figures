@inproceedings{ WOS:000404511900122,
Author = {Tchertchian, N. and Millet, D.},
Editor = {Takata, S and Umeda, Y and Kondoh, S},
Title = {Monitoring environmental performance during the design process of a
   complex system},
Booktitle = {24TH CIRP CONFERENCE ON LIFE CYCLE ENGINEERING},
Series = {Procedia CIRP},
Year = {2017},
