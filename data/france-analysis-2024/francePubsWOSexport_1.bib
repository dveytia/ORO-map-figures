@article{ WOS:000398910000010,
Author = {Dahmani, Ouahid and Bourguet, Salvy and Machmoum, Mohamed and Guerin,
   Patrick and Rhein, Pauline and Josse, Lionel},
Title = {Optimization and Reliability Evaluation of an Offshore Wind Farm
   Architecture},
Journal = {IEEE TRANSACTIONS ON SUSTAINABLE ENERGY},
