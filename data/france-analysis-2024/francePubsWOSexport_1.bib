@article{ WOS:000498398700039,
Author = {Flambard, Jorel and Amirat, Yassine and Feld, Gilles and Benbouzid,
   Mohamed and Ruiz, Nicolas},
Title = {River and Estuary Current Power Overview},
Journal = {JOURNAL OF MARINE SCIENCE AND ENGINEERING},
