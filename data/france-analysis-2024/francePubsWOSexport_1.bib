@article{ WOS:000615259900003,
Author = {Lemee, Colin and Navarro, Oscar and Restrepo-Ochoa, Diego and Mercier,
   Denis and Fleury-Bahi, Ghozlane},
Title = {Protective behaviors regarding coastal flooding risk in a context of
   climate change},
Journal = {ADVANCES IN CLIMATE CHANGE RESEARCH},
