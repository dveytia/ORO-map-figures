Author = {Pham, Huu-Tam and Bourgeot, Jean-Matthieu and Benbouzid, Mohamed},
Title = {Fault-tolerant finite control set-model predictive control for marine
   current turbine applications},
Journal = {IET RENEWABLE POWER GENERATION},
Year = {2018},
