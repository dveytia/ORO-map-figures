@article{ WOS:000261503400007,
Author = {Douvere, Fanny and Ehler, Charles N.},
Title = {New perspectives on sea use management: Initial findings from European
   experience with marine spatial planning},
Journal = {JOURNAL OF ENVIRONMENTAL MANAGEMENT},
