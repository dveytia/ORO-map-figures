@article{ WOS:000364452300003,
Author = {Ybert, Sebastien},
Title = {France Energies Marines: objectives and status of the French sea-trial
   test sites},
Journal = {HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU},
Year = {2015},
Number = {5},
Pages = {23-28},
Month = {OCT},
Abstract = {The development of Marine Renewable Energy (MRE) technologies requires
   the test of industrial scale prototypes in real conditions to validate
   their performance, reliability etc. prior to deploying commercial scale
   projects. MRE Test sites, the implementation of which was initiated in
   the UK with the EMEC in 2004, offer a pooled infrastructure to
   technology developers, hence sharing the cost of key infrastructure
   components over multiple devices, and a range of services aimed at
   supporting the developers in the various stages of technology
   development.
   In France, France Energies Marines, the research institute dedicated to
   MRE, is coordinating the development and operations of test sites
   offering the required infrastructure for wave energy, tidal current
   energy and floating offshore wind. The coordination of the various test
   sites at a national level offers a range of benefits to both the local
   partners involved in the development and operations of these sites and
   to the industrial developers with access to a large pool of highly
   skilled team, shared equipment and common policies and quality standards
   for example. France Energies Marines is also working to develop
   collaboration and cooperation at an international level with other test
   and research centres.},
