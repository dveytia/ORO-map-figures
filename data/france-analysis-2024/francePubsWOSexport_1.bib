@article{ WOS:000267602300010,
Author = {Brakni, Samira and Abriak, Nor Edine and Hequette, Arnaud},
Title = {Formulation of artificial aggregates from dredged harbour sediments for
   coastline stabilization},
Journal = {ENVIRONMENTAL TECHNOLOGY},
