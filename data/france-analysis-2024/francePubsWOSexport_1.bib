@article{ WOS:000343162800002,
Author = {Sedrati, Mouncef and Anthony, Edward J.},
Title = {Confronting coastal morphodynamics with counter-erosion engineering: the
   emblematic case of Wissant Bay, Dover Strait},
Journal = {JOURNAL OF COASTAL CONSERVATION},
Year = {2014},
Volume = {18},
Number = {5, SI},
