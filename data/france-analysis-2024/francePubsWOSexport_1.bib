@article{ WOS:000401483300004,
Author = {Rivera-Ingraham, Georgina A. and Lignot, Jehan-Herve},
Title = {Osmoregulation, bioenergetics and oxidative stress in coastal marine
   invertebrates: raising the questions for future research},
