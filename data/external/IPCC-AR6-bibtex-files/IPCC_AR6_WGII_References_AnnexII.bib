issn = {1758-6798},
journal = {Nature Climate Change},
keywords = {(wg1)},
mendeley-tags = {(wg1)},
number = {6},
pages = {469--477},
title = {{Future climate risk from compound events}},
translator = {C2465},
url = {https://doi.org/10.1038/s41558-018-0156-3},
volume = {8},
