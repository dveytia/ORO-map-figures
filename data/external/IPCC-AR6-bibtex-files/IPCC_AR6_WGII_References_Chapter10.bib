@article{RN1949,
   author = {Reilly, J. and Melillo, J. and Cai, Y. and Kicklighter, D. and Gurgel, A. and Paltsev, S. and Cronin, T. and Sokolov, A. and Schlosser, A.},
   title = {Using land to mitigate climate change: hitting the target, recognizing the trade-offs},
   journal = {Environ Sci Technol},
   volume = {46},
   number = {11},
   pages = {5672-9},
   note = {Reilly, John
Melillo, Jerry
Cai, Yongxia
Kicklighter, David
