@article{RN1924,
   author = {Jevrejeva, S. and Jackson, L. P. and Riva, R. E. and Grinsted, A. and Moore, J. C.},
   title = {Coastal sea level rise with warming above 2 degrees C},
   journal = {Proc Natl Acad Sci U S A},
   volume = {113},
   number = {47},
   pages = {13342-13347},
   note = {Jevrejeva, Svetlana
Jackson, Luke P
