@article{RN1820,
   author = {Yu, Rong and Zhai, Panmao and Chen, Yang},
   title = {Facing climate change-related extreme events in megacities of China in the context of 1.5 °C global warming},
   journal = {Current Opinion in Environmental Sustainability},
   volume = {30},
   pages = {75-81},
   abstract = {Studies on the effects of 1.5 °C global warming above pre-industrial levels, especially in megacities, are very limited. Here, we review recent progress in understanding climate change-related extreme temperature and precipitation events in China's megacities, and provide some potential options for adaptation. Studies have indicated that both global warming and urbanization could strengthen heatwave and extreme precipitation events. These extreme events have shown an uptrend in China's megacities and are projected to further increase in the future. Notably, the projected intensity of extreme temperature and precipitation events in China's three urban agglomerations is lower under 1.5 °C global warming compared with 2 °C. Furthermore, adaptation options to cope with the increasing risks of extreme weather and climate events are crucial for megacities due to the combined effects of global warming and urbanization.},
   DOI = {10.1016/j.cosust.2018.03.008},
   year = {2018},
   type = {Journal Article}
}

