@article{RN1992,
   author = {Liu, Y. and Yu, D. and Xun, B. and Sun, Y. and Hao, R.},
   title = {The potential effects of climate change on the distribution and productivity of Cunninghamia lanceolata in China},
   journal = {Environ Monit Assess},
   volume = {186},
   number = {1},
   pages = {135-49},
   note = {Liu, Yupeng
Yu, Deyong
Xun, Bin
Sun, Yun
