@article{RN1812,
   author = {Dulal, Hari Bansha},
   title = {Cities in Asia: how are they adapting to climate change?},
   journal = {Journal of Environmental Studies and Sciences},
   volume = {9},
   number = {1},
   pages = {13-24},
   abstract = {Asian cities are at the forefront of climate change impacts. Given the size of city economies, climate-induced extreme weather event losses are quite significant by developing and middle-income country standards. Together, the Asian cities of Guangzhou, Mumbai, Shenzen, Tianjin, Ho Chi Minh City, Kolkata, and Jakarta incurred $1520 million (USD) in annual loss to coastal flooding alone in 2005. Annual loss incurred by these cities from coastal flooding is projected to increase to $32,079 million by 2050. In the light of projected increase in climate-induced extreme weather events and associated loss, it is important to understand where Asian cities are on the climate adaptation-curve, what types of adaptation interventions are being pursued, and possible motivations behind selection of certain interventions, sectoral preferences, and the nature of interventions. We use publicly available data and information published between 2004 and 2014 to capture urban climate change adaptation interventions, their sectoral distribution, and nature. Results shed light on urban climate change adaptation progress in Asian cities and could be used in urban adaptation framework development, and inform methods necessary to implement urban climate change adaptation strategies and action plans.},
   keywords = {Asia
Asia-specific Ch 10
