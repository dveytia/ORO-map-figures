@article{RN2848,
   author = {Liss, Alexander and Wu, Ruiruo and Chui, Kenneth Kwan Ho and Naumova, Elena N.},
   title = {Heat-Related Hospitalizations in Older Adults: An Amplified Effect of the First Seasonal Heatwave},
   journal = {Sci. Rep.},
   volume = {7},
   pages = {39581},
   abstract = {Older adults are highly vulnerable to the detriment of extreme weather. The rapid non-linear increase in heat-related morbidity is difficult to quantify, hindering the attribution of direct effects of exposure on severe health outcomes. We examine the effects of ambient temperatures on heat-related hospitalizations (HH) among the elderly in presence of strong seasonality and by assessing the effects caused by the first and subsequent seasonal heatwaves. We empirically derived the thresholds for a heatwave episode in Boston MSA based on 16 years of daily observations. We compared the health risks of heatwaves using the proposed and four alternative definitions. 701 cases of HH in older residents of Boston area were examined using harmonic regression models, designed to capture the non-linear effects of ambient temperatures and heatwave episodes when the night-time temperature is above 65.5 °F for 3 consecutive nights. The overall relative risk of HH associated with a heatwave episode was 6.9 [95%CI:4.8-9.8]. The relative risk of HH associated with the first heatwave increases up to 13.3 [95%CI:7.4-24.0]. The risk declined to 3.7 [95%CI:2.4-5.8] for the subsequent heatwave. Four other commonly used heatwave definitions confirmed these findings. Public health actions have to target the first heatwave to maximize the impact of preventive measures.},
   ISSN = {2045-2322},
   DOI = {10.1038/srep39581},
   url = {http://dx.doi.org/10.1038/srep39581
https://www.ncbi.nlm.nih.gov/pubmed/28074921
https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5225426
https://doi.org/10.1038/srep39581},
   year = {2017},
   type = {Journal Article}
}

@article{RN1041,
   author = {Little, L. Richard and Hobday, Alistair J. and Parslow, John and Davies, Campbell R. and Grafton, R. Quentin},
   title = {Funding climate adaptation strategies with climate derivatives},
   journal = {Climate Risk Management},
   volume = {8},
   pages = {9-15},
   abstract = {Climate adaptation requires large capital investments that could be provided not only by traditional sources like governments and banks, but also by derivatives markets. Such markets would allow two parties with different tolerances and expectations about climate risks to transact for their mutual benefit and, in so doing, finance climate adaptation. Here we calculate the price of a derivative called a European put option, based on future sea surface temperature (SST) in Tasmania, Australia, with an 18 °C strike threshold. This price represents a quantifiable indicator of climate risk, and forms the basis for aquaculture industries exposed to the risk of higher SST to finance adaptation strategies through the sale of derivative contracts. Such contracts provide a real incentive to parties with different climate outlooks, or risk exposure to take a market assessment of climate change.},
