@article{RN1176,
   author = {Monge, J. J. and Parker, W. J. and Richardson, J. W.},
   title = {Integrating forest ecosystem services into the farming landscape: A stochastic economic assessment},
   journal = {J Environ Manage},
   volume = {174},
