@article{RN2840,
   author = {Ebi, Kristie L. and Boyer, Christopher and Ogden, Nicholas and Paz, Shlomit and Berry, Peter and Campbell-Lendrum, Diarmid and Hess, Jeremy J. and Woodward, Alistair},
   title = {Burning embers: synthesis of the health risks of climate change},
   journal = {Environ. Res. Lett.},
   volume = {16},
   number = {4},
   pages = {044042},
   abstract = {Since 2001, a synthesizing element in Intergovernmental Panel on Climate Change assessment reports has been a summary of how risks in a particular system could change with additional warming above pre-industrial levels, generally accompanied by a figure called the burning embers. We present a first effort to develop burning embers for climate change risks for heat-related morbidity and mortality, ozone-related mortality, malaria, diseases carried by Aedes sp., Lyme disease, and West Nile fever. We used an evidence-based approach to construct the embers based on a comprehensive global literature review. Projected risks for these health outcomes under 1.5 °C, 2 °C, and >2 °C of warming were used to estimate at what temperatures risk levels increased from undetectable to medium, high, and very high, from the pre-industrial baseline, under three adaptation scenarios. Recent climate change has likely increased risks from undetectable to moderate for heat-related morbidity and mortality, ozone-related mortality, dengue, and Lyme disease. Recent climate change also was assessed as likely beginning to affect the burden of West Nile fever. A detectable impact of climate change on malaria is not yet apparent but is expected to occur with additional warming. The risk for each climate-sensitive health outcome is projected to increase as global mean surface temperature increases above pre-industrial levels, with the extent and pace of adaptation expected to affect the timing and magnitude of risks. The embers may be an effective tool for informing efforts to build climate-resilient health systems including through vulnerability, capacity, and adaptation assessments and the development of national adaptation plans. The embers also can be used to raise awareness of future threats from climate change and advocate for mitigation actions to reduce the overall magnitude of health risks later this century and to expand current adaptation efforts to protect populations now.},
   ISSN = {1748-9326},
   DOI = {10.1088/1748-9326/abeadd},
   url = {https://iopscience.iop.org/article/10.1088/1748-9326/abeadd/meta
https://iopscience.iop.org/article/10.1088/1748-9326/abeadd
https://iopscience.iop.org/article/10.1088/1748-9326/abeadd/pdf
http://dx.doi.org/10.1088/1748-9326/abeadd},
   year = {2021},
   type = {Journal Article}
}

