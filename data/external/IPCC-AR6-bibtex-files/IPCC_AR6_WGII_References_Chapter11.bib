@article{RN974,
   author = {Kunapo, Joshphar and Fletcher, Tim D. and Ladson, Anthony R. and Cunningham, Luke and Burns, Matthew J.},
   title = {A spatially explicit framework for climate adaptation},
   journal = {Urban Water Journal,},
   volume = {15},
   number = {2},
   pages = {159-166},
   abstract = {AbstractCities increasingly confront climate change-related problems of flooding, urban heat island effects, and the impact of drought on vegetation. There is a need for urban water and infrastructure planners to be able to identify vulnerable areas and to design, compare and evaluate interventions to address these problems. In response to this need, we developed the Integrated Climate Adaptation Model (ICAM), which is a Web-GIS tool based on a spatially explicit framework. ICAM has a user-friendly interface for use by a wide range of urban planning and design professionals. The tool is built on high-resolution spatial datasets, allowing users to identify critical areas that may be impacted by sea-level rise, drought, flooding, temperature increases and threats to tree heath and to consider the benefits of various grey and green infrastructure interventions. The tool is suitable for planning and evaluating interventions and to identify pathways for further desktop modelling.},
   ISSN = {1573-062X},
   DOI = {10.1080/1573062X.2018.1424216},
