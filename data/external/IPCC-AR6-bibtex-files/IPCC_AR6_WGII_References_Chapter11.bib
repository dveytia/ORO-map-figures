@article{RN1591,
   author = {Wallace, Kiri Joy and Clarkson, Bruce D.},
   title = {Urban forest restoration ecology: a review from Hamilton, New Zealand},
   journal = {null},
   volume = {49},
   number = {3},
   pages = {347-369},
   abstract = {ABSTRACTRestoration of urban forest improves quality of life for city residents and is important for boosting native biodiversity. However, the scientific knowledge required to inform successful restoration is largely lacking for urban forest ecosystems, which differ from rural forests. Here we review two decades of urban forest restoration research in Hamilton, New Zealand and summarise key findings across seven major ecological topics: i. species traits, filters and thresholds, ii. species richness and target ecosystems, iii. tree regeneration, iv. seed banks and seed rain, v. seed predation, vi. enrichment planting and vii. restored forest function. We then discuss general urban restoration principles that increase the efficacy of urban restoration efforts including restore to a minimum of 10% indigenous cover in cities, develop a step-wise restoration plan and prioritise partner engagement. The purpose of this review is to aid urban forest restoration across New Zealand and globally.},
   ISSN = {0303-6758},
   DOI = {10.1080/03036758.2019.1637352},
   url = {https://doi.org/10.1080/03036758.2019.1637352
